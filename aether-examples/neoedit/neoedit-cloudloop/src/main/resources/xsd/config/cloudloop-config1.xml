<?xml version="1.0" encoding="UTF-8"?>
<cloudloop-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="cloudloop-config.xsd">
  <logger-config-file>logger-config-file</logger-config-file>
  <adapters>
    <adapter impl="" name="" type="storage"/>
  </adapters>
  <stores>
    <default-store>default-store</default-store>
    <store>
      <name>name</name>
      <adapter>adapter</adapter>
      <encrypted>true</encrypted>
      <property name="">property</property>
    </store>
  </stores>
  <encryption>
    <cipher>cipher</cipher>
    <key-length>0</key-length>
    <preferred-key>preferred-key</preferred-key>
    <key-directory>key-directory</key-directory>
  </encryption>
</cloudloop-config>
